<!DOCTYPE html>

<title>Ticket</title>

{% block body %}
    <h1>Ticket</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Ticketid</th>
                <td>{{ ticket.ticketid }}</td>
            </tr>
            <tr>
                <th>Bus_id</th>
                <td>{{ ticket.busId }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ ticket.prix }}</td>
            </tr>
            <tr>
                <th>DateAchat</th>
                <td>{{ ticket.dateAchat ? ticket.dateAchat|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_ticket_index') }}">back to list</a>

    <a href="{{ path('app_ticket_edit', {'ticketid': ticket.ticketid}) }}">edit</a>

    {{ include('ticket/_delete_form.html.twig') }}
{% endblock %}
