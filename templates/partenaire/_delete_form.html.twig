<form method="post" action="{{ path('app_partenaire_delete', {'idPartenaire': partenaire.idPartenaire}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce partenaire ?');" style="display: inline-block">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ partenaire.idPartenaire) }}">
    <button class="btn btn-delete">
        <i class="fas fa-trash-alt me-2"></i>Supprimer
    </button>
</form>